{
  "name": "leadpersona",
  "description": "LeadPersona | Your AI Leader For Business Outreach",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "source": "./src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "prestart": "yarn style",
    "start": "parcel ./src/index.html --dist-dir ./public/dist",
    "prebuild": "yarn style && del \"public/dist\"",
    "build": "parcel build ./src/index.html --no-content-hash --no-source-maps --dist-dir ./public/dist",
    "style": "npx tailwindcss -i ./src/input.css -o ./src/tailwindcss-output.css -m",
    "style:watch": "yarn style --watch",
    "lint": "prettier --no-semi --tab-width 2 --trailing-comma es5 --single-quote true -w ."
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-frame-component": "^5.2.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/chrome": "^0.0.236",
    "@types/node": "^20.2.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "parcel": "^2.8.3",
    "postcss": "^8.4.16",
    "postcss-nested": "^5.0.6",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  }
}
