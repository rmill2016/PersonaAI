{
  "name": "leadpersona",
  "description": "LeadPersona | Your AI Leader For Business Outreach",
  "version": "0.2.0",
  "license": "MIT",
  "source": "./src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "prestart": "yarn style",
    "start": "parcel --dist-dir ./public/dist",
    "prebuild": "yarn style && del \"./public/dist\"",
    "build": "parcel build src/index.html --no-content-hash --dist-dir ./public/dist --no-source-maps",
    "style": "npx tailwindcss -i ./src/input.css -o ./src/tailwindcss-output.css",
    "style:watch": "yarn style -- --watch",
    "lint": "prettier --no-semi --tab-width 2 --trailing-comma es5 --single-quote true -w ."
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-frame-component": "^5.2.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/chrome": "^0.0.236",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "parcel": "^2.8.3",
    "parcel-namer-without-hash": "^0.0.1",
    "postcss": "^8.4.16",
    "postcss-nested": "^5.0.6",
    "process": "^0.11.10",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  }
}
