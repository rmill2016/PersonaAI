{
  "short_name": "LeadPersona",
  "name": "LeadPersona | Your AI Leader For Business Outreach",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["dist/index.cb601c27.css"],
      "js": ["dist/index.29496aea.js", "dist/index.a09bb8b2.js"]
    }
  ],
  "permissions": ["storage", "scripting", "tabs", "activeTab"],
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "120": "img/Logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["img/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
